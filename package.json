{
  "name": "animation-canvas",
  "version": "1.0.0",
  "description": "A React canvas component tailored for animations",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development --port 8081 --hot --host 0.0.0.0",
    "lint": "eslint src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "jest": {
    "moduleNameMapper": {
      ".+\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test/module.mock.js",
      "\\.(css|less)$": "<rootDir>/src/test/module.mock.js"
    }
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor",
    "rules": {
      "no-unused-vars": "warn",
      "react/sort-comp": "off",
      "lines-around-comment": "off",
      "react/prefer-stateless-function": "off"
    }
  },
  "eslintIgnore": [
    "build/*"
  ],
  "author": "Tobias Morell <tobiasmorell@hotmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/runtime": "^7.12.5",
    "@rmwc/chip": "^6.1.4",
    "@rmwc/circular-progress": "^6.1.4",
    "@rmwc/snackbar": "^6.1.4",
    "@rmwc/typography": "^6.1.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.11.0",
    "eslint-config-synacor": "^3.0.5",
    "file-loader": "^6.1.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "if-env": "^1.0.0",
    "jest": "^26.6.3",
    "style-loader": "^2.0.0",
    "victor": "^1.1.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
